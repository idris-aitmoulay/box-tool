    <name>Box Tool</name>
    <vendor>Idris AIT MOULAY</vendor>
    <depends>com.intellij.modules.platform</depends>
    <extensions defaultExtensionNs="com.intellij">
        <projectConfigurable
                parentId="tools"
                instance="com.github.idrisaitmoulay.boxtool.settings.BoxToolProjectSettings"
                id="com.github.idrisaitmoulay.boxtool.settings.BoxToolProjectSetting"
                displayName="Box Tool"
        />
